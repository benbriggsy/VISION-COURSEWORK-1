function [feat] = get_tiny_images2(image_paths, sqrtD)
    feat = zeros(length(image_paths), sqrtD*sqrtD);
    for i = 1 : length(image_paths)
        img = imread(image_paths{i});
        img = imresize(img,[sqrtD sqrtD]);
        imshow(img);
        img = img(:,:,3);
        size(img)
        feat(i, :) = img(:);    
    end
end